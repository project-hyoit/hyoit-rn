{
  "expo": {
    "name": "hyoit-rn",
    "slug": "hyoit-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "hyoitrn",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": { "supportsTablet": true },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": { "typedRoutes": true }
  }
}
